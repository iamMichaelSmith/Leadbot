AWS_REGION=us-east-1
LEADS_TABLE=MusicLibraryLeads
PAGES_TABLE=MusicLibraryPages
USER_AGENT=MusicLibraryLeadFinder/1.0
REQUEST_TIMEOUT=20
SLEEP_BETWEEN_REQUESTS=2.0
MAX_PAGES_PER_RUN=120
MAX_LEADS_PER_RUN=0
MAX_PAGES_PER_DOMAIN=10
MAX_LINKS_PER_PAGE=40
ALLOW_EXTERNAL_DOMAINS=false
EXPORT_LEADS_FILE=leads_export.jsonl
REQUIRE_SAME_DOMAIN_FORM=1
MIN_ROLE_CONFIDENCE=0
LIBRARIES_ONLY=0
MIN_LIBRARY_CONFIDENCE=60
VISITED_CACHE_ENABLED=1
VISITED_CACHE_TABLE=MusicLibraryPages
VISITED_CACHE_TTL_HOURS=0
# Optional: set to DynamoDB Local, e.g. http://localhost:8000
DYNAMODB_ENDPOINT_URL=

# Discovery (Brave or Serper)
DISCOVERY_ENABLED=0
DISCOVERY_PROVIDER=brave
DISCOVERY_PROVIDERS=brave,serper
DISCOVERY_QUERIES_FILE=queries.txt
DISCOVERY_MAX_URLS=80
DISCOVERY_PER_QUERY=10
DISCOVERY_BATCH_SIZE=50
DISCOVERY_STATE_FILE=discovery_state.json
DISCOVERY_DAILY_QUERY_LIMIT_BRAVE=2000
DISCOVERY_DAILY_QUERY_LIMIT_SERPER=2500
BRAVE_API_KEY=
SERPER_API_KEY=
DISCO_PORTFOLIO_LINK=
